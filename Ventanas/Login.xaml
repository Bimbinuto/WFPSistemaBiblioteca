                            </StackPanel>
                        </Border>
                        <ControlTemplate.Triggers>
                            <!--  BORDES CON ANIMACION GRADIENTE  -->
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter TargetName="Icon2" Property="Fill" Value="#fcfcda" />
                                <Trigger.EnterActions>
                                    <BeginStoryboard>
                                        <Storyboard>
                                            <DoubleAnimation
                                                Storyboard.TargetName="GS1"
                                                Storyboard.TargetProperty="(GradientStop.Offset)"
                                                To="0.0"
                                                Duration="0:0:0.1" />
                                            <DoubleAnimation
                                                Storyboard.TargetName="GS2"
                                                Storyboard.TargetProperty="(GradientStop.Offset)"
                                                To="0.5"
                                                Duration="0:0:0.1" />
                                            <ColorAnimation
                                                Storyboard.TargetName="GS1"
                                                Storyboard.TargetProperty="(GradientStop.Color)"
                                                To="#ffdece"
                                                Duration="0:0:0.1" />
                                            <ColorAnimation
                                                Storyboard.TargetName="GS2"
                                                Storyboard.TargetProperty="(GradientStop.Color)"
                                                To="#a62520"
                                                Duration="0:0:0.1" />
                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.EnterActions>
                                <Trigger.ExitActions>
                                    <BeginStoryboard>
                                        <Storyboard FillBehavior="Stop">
                                            <DoubleAnimation
                                                Storyboard.TargetName="GS1"
                                                Storyboard.TargetProperty="(GradientStop.Offset)"
                                                To="-0.1"
                                                Duration="0:0:0.2" />
                                            <DoubleAnimation
                                                Storyboard.TargetName="GS2"
                                                Storyboard.TargetProperty="(GradientStop.Offset)"
                                                To="1.0"
                                                Duration="0:0:0.2" />
                                            <ColorAnimation
                                                Storyboard.TargetName="GS1"
                                                Storyboard.TargetProperty="(GradientStop.Color)"
                                                To="#ffdece"
                                                Duration="0:0:0.2" />
                                            <ColorAnimation
                                                Storyboard.TargetName="GS2"
                                                Storyboard.TargetProperty="(GradientStop.Color)"
                                                To="#ffdece"
                                                Duration="0:0:0.2" />

                                        </Storyboard>
                                    </BeginStoryboard>
                                </Trigger.ExitActions>
                            </Trigger>
                            <Trigger Property="IsFocused" Value="True">
                                <Setter Property="BorderBrush" Value="{DynamicResource ColorBordeUsernameConCursor}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--  Boton de Login  -->
        <Style x:Key="ButtonLogin" TargetType="{x:Type Button}">
            <Setter Property="Width" Value="100" />
            <Setter Property="Height" Value="35" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Margin" Value="0,50,0,20" />
            <Setter Property="Background" Value="#50868686" />
            <Setter Property="Foreground" Value="White" />
            <Setter Property="BorderBrush" Value="White" />
            <Setter Property="BorderThickness" Value="0.8" />
            <Setter Property="FontSize" Value="15" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="#80868686" />
                            </Trigger>
                            <Trigger Property="IsPressed" Value="True">
                                <Setter Property="Background" Value="{DynamicResource ColorBordeUsernameConCursor}" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--  Boton con animacion ??  -->
        <Style x:Key="ButtonInteresting" TargetType="{x:Type Button}">
            <Setter Property="RenderTransformOrigin" Value="0.5, 0.5" />
            <Setter Property="RenderTransform">
                <Setter.Value>
                    <ScaleTransform />
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <EventTrigger RoutedEvent="MouseEnter">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation
                                x:Name="ForAxixX"
                                Storyboard.TargetProperty="RenderTransform.ScaleX"
                                To="1.2"
                                Duration="0:0:0.1">
                                <!--<DoubleAnimation.EasingFunction>
                                    <BounceEase Bounces="2" Bounciness="2"/>
                                </DoubleAnimation.EasingFunction>-->
                                <DoubleAnimation.EasingFunction>
                                    <CubicEase EasingMode="EaseIn" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation
                                x:Name="ForAxisY"
                                Storyboard.TargetProperty="RenderTransform.ScaleY"
                                To="1.2"
                                Duration="0:0:0.1">
                                <!--<DoubleAnimation.EasingFunction>
                                    <BounceEase Bounces="2" Bounciness="3"/>
                                </DoubleAnimation.EasingFunction>-->
                                <DoubleAnimation.EasingFunction>
                                    <CubicEase EasingMode="EaseIn" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
                <EventTrigger RoutedEvent="MouseLeave">
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation
                                Storyboard.TargetProperty="RenderTransform.ScaleX"
                                To="1"
                                Duration="0:0:0.2" />
                            <DoubleAnimation
                                Storyboard.TargetProperty="RenderTransform.ScaleY"
                                To="1"
                                Duration="0:0:0.2" />
                        </Storyboard>
                    </BeginStoryboard>
                </EventTrigger>
            </Style.Triggers>
        </Style>

        <!--  Borde de la ventana Login  -->
        <Style x:Key="BorderWindow" TargetType="{x:Type Border}">
            <Setter Property="BorderBrush">
                <Setter.Value>
                    <LinearGradientBrush StartPoint="0.1, 1" EndPoint="1.1, 0">
                        <GradientStop Offset="0" Color="#840007" />
                        <GradientStop Offset="1" Color="#fcfcda" />
                    </LinearGradientBrush>
                </Setter.Value>
            </Setter>
        </Style>

        <!--  Estilo del boton Cerrar y mas [general]  -->
        <Style x:Key="StyleButtonIcons" TargetType="{x:Type Button}">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Width" Value="43" />
            <Setter Property="Height" Value="34" />
            <Setter Property="HorizontalAlignment" Value="Right" />
            <Setter Property="BorderBrush" Value="AliceBlue" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Padding" Value="10" />
            <Setter Property="ToolTip" Value="Cerrar" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <Path
                                x:Name="IconBar"
                                Width="11"
                                Height="11"
                                Data="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Tag}"
                                Fill="White"
                                Stretch="Uniform"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Center" />
                        </Border>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsMouseOver" Value="True">
                                <Setter Property="Background" Value="#40ffffff" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <!--  Animacion del Login Successful  -->
        <Storyboard x:Key="animacionLogin">
            <DoubleAnimation
                Storyboard.TargetName="border"
                Storyboard.TargetProperty="Width"
                From="0"
                To="696"
                Duration="0:0:0.5"
                AccelerationRatio="0.3"
                DecelerationRatio="0.6" />
            <DoubleAnimation
                Storyboard.TargetName="txtBienvenido"
                Storyboard.TargetProperty="Opacity"
                From="0"
                To="1"
                BeginTime="0:0:0.6"
                Duration="0:0:0.5" />
            <DoubleAnimation
                Storyboard.TargetName="imgUsuario"
                Storyboard.TargetProperty="Opacity"
                From="0"
                To="1"
                BeginTime="0:0:0.6"
                Duration="0:0:0.5" />
            <DoubleAnimation
                Storyboard.TargetName="txtUsuario"
                Storyboard.TargetProperty="Opacity"
                From="0"
                To="1"
                BeginTime="0:0:0.6"
                Duration="0:0:0.5" />
        </Storyboard>
    </Window.Resources>

    <Border BorderThickness="2" Style="{StaticResource BorderWindow}">
        <Border.Background>
            <LinearGradientBrush StartPoint="0.6,0.3" EndPoint="0,0.7">
                <GradientStop Color="#ffbf75" Offset="1" />
                <GradientStop Color="#d0251111" Offset="0" />
            </LinearGradientBrush>
        </Border.Background>

        <Grid Background="Transparent">

            <!--  lienzo de bienvenida  -->
            <Canvas Panel.ZIndex="1">
                <Border
                    x:Name="border"
                    Width="0"
                    Height="446"
                    HorizontalAlignment="Left">
                    <Border.Background>
                        <LinearGradientBrush StartPoint="1,0.2" EndPoint="0,0.1">
                            <GradientStop Color="#f0ffbf75" Offset="0" />
                            <GradientStop Color="#f0000000" Offset="1" />
                        </LinearGradientBrush>
                    </Border.Background>

                    <StackPanel
                        Orientation="Vertical"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center">
                        <TextBlock
                            x:Name="txtBienvenido"
                            Text="Bienvenido!"
                            HorizontalAlignment="Center"
                            FontSize="39"
                            Opacity="0"
                            FontWeight="Light"
                            FontFamily="Calibri"
                            Foreground="White"
                            Margin="0,0,0,25" />
                        <Ellipse
                            x:Name="imgUsuario"
                            Width="150"
                            Height="150"
                            StrokeThickness="3"
                            Stroke="#fcfcda"
                            Margin="0,0,0,10"
                            Opacity="0"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center">
                            <Ellipse.Fill>
                                <ImageBrush ImageSource="/Recursos/avatar.jpg" />
                            </Ellipse.Fill>
                        </Ellipse>


                        <TextBlock
                            x:Name="txtUsuario"
                            HorizontalAlignment="Center"
                            Text="Administrador"
                            FontSize="24"
                            Margin="0"
                            Opacity="0"
                            FontWeight="Light"
                            FontFamily="Calibri"
                            Foreground="White" />
                    </StackPanel>
                    <!--<Rectangle Width="200" Height="200">
                        <Rectangle.Fill>
                            <RadialGradientBrush GradientOrigin="0.5,0.5" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5">
                                <RadialGradientBrush.GradientStops>
                                    <GradientStop Color="Yellow" Offset="0" />
                                    <GradientStop Color="Red" Offset="0.25" />
                                    <GradientStop Color="Blue" Offset="0.75" />
                                    <GradientStop Color="Transparent" Offset="1" />
                                </RadialGradientBrush.GradientStops>
                            </RadialGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>-->
                </Border>
            </Canvas>

            <Grid Grid.Column="0">
                <Image
                    Margin="0,0,-350,0"
                    Source="/Recursos/Library02.jpg"
                    Stretch="UniformToFill">
                    <Image.OpacityMask>
                        <LinearGradientBrush StartPoint="0.42, -0.12" EndPoint=" 0.12, -0.02">
                            <GradientStop Offset="0" Color="Transparent" />
                            <GradientStop Offset="1" Color="Black" />
                        </LinearGradientBrush>
                    </Image.OpacityMask>
                </Image>
            </Grid>

            <Grid Grid.Column="1">

                <StackPanel Grid.Column="1">

                    <Button
                        Margin="0,0,0,0"
                        Style="{StaticResource StyleButtonIcons}"
                        Tag="{StaticResource IconCloseLogin}"
                        Click="BtnCerrarLogin" />

                    <Label
                        Margin="0,20,0,20"
                        Padding="20"
                        HorizontalAlignment="Center"
                        Content="Iniciar Sesion"
                        FontFamily="Calibri light"
                        FontSize="36"
                        FontWeight="UltraBold"
                        Foreground="White" />

                    <Label
                        Margin="50,0,0,0"
                        HorizontalAlignment="Left"
                        Content="Username:"
                        FontSize="16"
                        Foreground="White"
                        FontFamily="Calibri" />

                    <TextBox
                        Style="{StaticResource TextBoxStyleUsername}"
                        Text="@username"
                        x:Name="txtUsername" />

                    <Label
                        Margin="50,10,0,0"
                        HorizontalAlignment="Left"
                        Content="Password:"
                        FontSize="16"
                        Foreground="White"
                        FontFamily="Calibri" />

                    <PasswordBox
                        Style="{StaticResource TextBoxStylePassword}"
                        Password="@password"
                        x:Name="txtPassword" />

                    <Button
                        Content="acceder"
                        Style="{StaticResource ButtonLogin}"
                        x:Name="btnAcceder"
                        Click="btnLogin" />

                    <Button
                        Width="150"
                        Height="30"
                        Content="forgot your password?"
                        FontSize="14"
                        Style="{DynamicResource ButtonInteresting}"
                        Visibility="Hidden" />

                    <Label
                        x:Name="lbTest"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Center"
                        Margin="-10"
                        Foreground="White" />
                </StackPanel>
            </Grid>

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>

        </Grid>
    </Border>
</Window>
